.home-container
	position relative
	min-height 100vh
	.home-content
		position relative
		margin 10vh 10vw
		min-height 80vh
		width 80vw
		section
			&.grid-container
				margin-top 10%
				min-height 100vh
				position relative
				margin-bottom 20vh
				md-grid-list
					transform scale(.9) !important
					md-grid-tile
						cursor pointer
						.contenido
							position relative
							width 100%
							height 100%
							overflow hidden
							&>*
								transition(500ms , 0)
							.capa
								width 0%
								height 100%
								background-color rgba(255, 255, 255, .4)
								opacity .9
								position absolute
								z-index 2
								transform skew(-50deg, 0)
								left -50%
							.imagen
								background-repeat no-repeat
								background-size cover
								background-position center

							md-grid-tile-footer
								bottom -50px
								height 80px
								background-color inherit
								display flex
								justify-content flex-start
								align-items flex-start
								opacity 0
								p
									font-weight 800
							.ir
								position absolute
								bottom 1px
								width 100%
								height 30px
								.md-button
									min-height 0px
									height 25px
									background-color verde
									margin 0px
									min-width 0px
									border-radius 30px
									display flex
									justify-content flex-start
									line-height 25px
									color white
									text-transform inherit
									padding 0 5px
									margin 0 5px

									md-icon
										transition(500ms , 0)
										margin 0px
										color white

						&:hover
							.capa
								animation destello 500ms infinite;
							.imagen
								transform scale(1.2)
							md-grid-tile-footer
								bottom 0px
								opacity 1
								padding 0 10px
								background-color rgba(0, 0, 0, .2)



							.ir
								.md-button
									md-icon
										margin-left 10px

